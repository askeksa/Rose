proc sun n
	when n
		when (n & 7) == 0
			fork sunbeam n
		done
		draw
		move 2
		turn 5
		wait 1
		fork sun n-1
	done

proc sunbeam n
	turn ~64
	move 10
	wait 180-n*2
	fork streg x y dir 1.5 1 8

proc smoke c n
	when n
		draw
		move 2
		defy turn ~c + sine(n/50)*3
		wait 0.5
		fork smoke c n-1
	done

proc streg x0 y0 d step l t
	jump x0 y0
	face d
	temp s = step*step
	size s
	defy seed x*y
	temp n = (l/step*16)&~1
	fork stregstep d step t*step/l/16 n

proc stregstep d step w n
	when n
		draw
		wait w
		move step
		temp rel = dir-d
		defy turn (rand-0.4-rel*0.0078125)*5
		fork stregstep d step w n-1
	done


