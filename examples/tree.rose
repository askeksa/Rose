
plan
	0:112 1:421 2:F00

proc main
	move 176
	turn 64
	move 250
	turn 128
	seed 42
	fork tree 8 30

proc tree s n
	when s > 0
		size s tint 1 draw
		move 2
		wait 1
		when n > 0
			fork tree s n-1
		else
			temp a = 10 + rand*40
			turn a
			fork tree s-1 s*3
			turn a * ~2
			fork tree s-1 s*3
		done
	else
		size 5 tint 2 draw
	done
