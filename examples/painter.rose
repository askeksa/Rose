proc wipe a d
	jump 176 140
	face a
	move 256
	turn 87*d
	fork wipe2 21 d

proc wipe2 n d
	fork cone
	turn 4*d
	wait 1
	when n fork wipe2 n-1 d done

proc cone
	move 32
	fork conesec 2 0.0625 44

proc conesec s f n
	size s draw
	move s
	when n fork conesec s+s*f f n-1 done

proc lineswipe a
	fork linewipe a
	wait 20
	fork linewipe a+128

proc linewipe a
	jump 176 140
	face a
	move 256
	turn 64
	move 208
	turn 64
	fork lines 20-3 11 40+6 64 7

proc lines s m l d n
	when n
		fork line s m 0.5 l
		turn 64
		move d
		turn ~64
		fork lines s m l d n-1
	done

proc line s m w n
	when n
		size s draw
		move m
		wait w
		fork line s m w n-1
	done

proc place_splats b n
	temp a = rand
	fork place_splat b
	wait 24
	when n > 0
		fork place_splats b n-1
	done

proc place_splat b
	face rand*16*8-128
	temp dist = rand*32*4+64
	move dist
	tint (rand*3+b)&3
	fork splats 4

proc splats n
	temp _size = rand*12+4
	size _size
	draw
	
	temp d = dir
	turn rand*16*16
	move rand*32
	face d

	when rand>0
		fork run _size*0.3
	done

	when n > 0
		fork splats n-1
	done

proc run _size
	move _size
	face dir - (dir-64)*0.4 + (rand-0.4)*_size*8.5
	draw
	size _size
	#wait 1
	when _size < 2
		wait 4
	done
	when _size>1
		fork run _size-0.1
	done
